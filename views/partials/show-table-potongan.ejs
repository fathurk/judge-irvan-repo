<% if (data.Employees) { %>
  <% let fee = 0 %>
  <table border=1>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Education</th>
        <th>Position</th>
        <th>Salary</th>
        <th>Action</th>
    </tr>
    <% data.Employees.forEach(each => { %>
      <% fee += each.salary %>
        <tr>
            <td><%= each.firstName %></td>
            <td><%= each.lastName %></td>
            <td><%= each.age %></td>
            <td><%= each.education %></td>
            <td><%= each.position %></td>
            <td><%= each.salaryRp %></td>
            <td>
              <a class="btn btn-info" href="/stores/<%=data.id%>/employees/<%=each.id%>/edit" role="button">Edit</a>
              <a class="btn btn-info" href="/stores/<%=data.id%>/employees/<%=each.id%>/delete" role="button">Delete</a>
            </td>
        </tr>
    <% }) %>
</table>